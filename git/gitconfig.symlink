[user]
  name = Sam McIngvale
  email = smcingvale@icloud.com
[github]
  user = smcingvale
[core]
  excludesfile = ~/.gitignore_global
[alias]
  tree = log --oneline --decorate --graph --color --date=short
  stat = status -s
[color]
  # Use colors in Git commands when available.
  # This is the default in Git >= 1.8.4.
  ui = auto
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
